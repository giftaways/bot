{
  "name": "giftaways.bot",
  "version": "1.0.0",
  "description": "Clustered and sharded client for Giftaways",
  "main": "dist/cluster.js",
  "scripts": {
    "dev": "npm run build && node dist/cluster.js",
    "start": "node dist/cluster.js",
    "build": "tsc",
    "format": "prettier --write src --ignore-unknown"
  },
  "license": "GPLV3",
  "prettier": {
    "trailingComma": "es5",
    "semi": true,
    "tabWidth": 2,
    "singleQuote": false
  },
  "dependencies": {
    "discord-hybrid-sharding": "^2.1.3",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
